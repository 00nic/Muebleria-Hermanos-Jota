# Backend env example for Mueblería Hermanos Jota
# Copy to .env and fill with your values

# MongoDB connection string (Atlas or local)
DB_CONNECTION_STRING=mongodb+srv://<user>:<password>@<cluster>/<database>?retryWrites=true&w=majority

# Server port (optional - defaults to 3001)
PORT=3001

#URL del front en producción y local para que el servidor acepte requests de ambos --
FRONTEND_URL=https://tuapp.vercel.app
FRONTEND_URL_LOCAL=http://localhost:3000
